{
  "version": 2,
  "buildCommand": "NODE_OPTIONS='--openssl-legacy-provider' npm run build",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "nodeVersion": "18.x",
        "buildCommand": "NODE_OPTIONS='--openssl-legacy-provider' npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}

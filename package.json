{
  "name": "zero-particle-svg",
  "version": "1.0.0",
  "description": "Interactive SVG particles using Three.js",
  "keywords": [
    "three.js",
    "webpack",
    "glsl",
    "svg"
  ],
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && webpack -p --config config/prod.js",
    "build:unix": "NODE_OPTIONS=--openssl-legacy-provider webpack -p --config config/prod.js",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && webpack-dev-server --config config/dev.js",
    "start:unix": "NODE_OPTIONS=--openssl-legacy-provider webpack-dev-server --config config/dev.js"
  },
  "dependencies": {
    "browser-detect": "^0.2.28",
    "gsap": "^2.0.2",
    "three": "^0.98.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.52",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-rc.1",
    "@babel/polyfill": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.52",
    "babel-loader": "^8.0.0-beta.4",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "glslify": "^6.2.1",
    "glslify-import-loader": "^0.1.2",
    "glslify-loader": "^1.0.2",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": ">=3.1.11",
    "webpack-merge": "^4.1.3"
  }
}
